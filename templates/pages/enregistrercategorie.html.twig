{% extends 'base.html.twig' %}


{% block title %}{{ parent() }} : enregistrer catégorie{% endblock %}

{% block body %}
{{ parent() }}
    
{{ form_start(form) }}
    <fieldset class="uk-fieldset">
        <legend class="uk-legend">Enregistrement d'une catégorie</legend>

        <div class="uk-input">
            {{ form_row(form.nom) }}
            <button class="uk-button uk-button-primary">Enregistrement</button>
        </div>
        
    </fieldset>
{{ form_end(form) }}

{% endblock %}



